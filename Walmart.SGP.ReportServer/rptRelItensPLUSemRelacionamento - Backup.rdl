        </Field>
        <Field Name="cdUPC">
          <DataField>cdUPC</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="dsStatus">
          <DataField>dsStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsSGP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT ID.cditem
	 , ID.dsItem AS dsItem1
	 , ID.dsHostItem AS dsItem2
	 , ID.dsTamanhoItem
	 , ID.cdPLU
	 , ID.cdUPC
	 , CASE ID.tpStatus
		   WHEN 'A' THEN
			   'Ativo'
		   WHEN 'I' THEN
			   'Inativo'
		   WHEN 'D' THEN
			   'Deletado'
	   END AS dsStatus

FROM
	ItemDetalhe ID (NOLOCK)
	INNER JOIN Departamento DP (NOLOCK)
		ON ID.IDDepartamento = DP.IDDepartamento
	LEFT JOIN RelacionamentoItemPrincipal RIP (NOLOCK)
		ON RIP.IDItemDetalhe = ID.IDItemDetalhe
	LEFT JOIN RelacionamentoItemSecundario RIS (NOLOCK)
		ON RIS.IDItemDetalhe = ID.IDItemDetalhe

WHERE
	DP.IDDepartamento = @IDDepartamento
	AND isnull(ID.cdPLU, 0) != 0
	AND (DP.blPerecivel = 'S'
	AND DP.blAtivo = 1)
	AND RIP.IDItemDetalhe IS NULL
	AND RIS.IDItemDetalhe IS NULL

ORDER BY
	ID.dsItem</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@IDDepartamento">
            <Value>=Parameters!IDDepartamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
