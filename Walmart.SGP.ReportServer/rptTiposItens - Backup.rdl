  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="cdDsDepartamento">
          <DataField>cdDsDepartamento</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cdItem">
          <DataField>cdItem</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="dsItem">
          <DataField>dsItem</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dsHostItem">
          <DataField>dsHostItem</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cdFornecedor">
          <DataField>cdFornecedor</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="nmFornecedor">
          <DataField>nmFornecedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cdPLU">
          <DataField>cdPLU</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="cdUPC">
          <DataField>cdUPC</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="tpVinculado">
          <DataField>tpVinculado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tpManipulado">
          <DataField>tpManipulado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tpReceituario">
          <DataField>tpReceituario</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="vlFatorConversao">
          <DataField>vlFatorConversao</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="pcRendimento">
          <DataField>pcRendimento</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="dsStatus">
          <DataField>dsStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsSGP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT (cast(DP.cdDepartamento AS VARCHAR) + ' - ' + cast(DP.dsDepartamento AS VARCHAR)) AS cdDsDepartamento  
     , ID.cdItem  
     , ID.dsItem  
     , ID.dsHostItem  
     , FN.cdFornecedor  
     , FN.nmFornecedor  
     , ID.cdPLU
     , ID.cdUPC
     , CASE ID.tpVinculado  
     WHEN 'S' THEN  
      'Saída'  
     WHEN 'E' THEN  
      'Entrada'  
    END tpVinculado  
     , CASE ID.tpManipulado  
     WHEN 'P' THEN  
      'Pai'  
     WHEN 'D' THEN  
      'Derivado'  
    END tpManipulado  
     , CASE ID.tpReceituario  
     WHEN 'T' THEN  
      'Transformado'  
     WHEN 'I' THEN  
      'Insumo'  
    END tpReceituario  
     , ID.vlFatorConversao  
     , CASE  
     WHEN (ID.tpReceituario IS NOT NULL) THEN  
      RIP.pcRendimentoReceita  
     WHEN (ID.tpManipulado IS NOT NULL) THEN  
      RIS.pcRendimentoDerivado  
    END pcRendimento  
     , CASE ID.tpStatus  
     WHEN 'A' THEN  
      'Ativo'  
     WHEN 'I' THEN  
      'Inativo'  
     WHEN 'D' THEN  
      'Deletado'  
    END AS dsStatus  
FROM  
 ItemDetalhe ID (NOLOCK)  
 INNER JOIN Departamento DP (NOLOCK)  
  ON ID.IDDepartamento = DP.IDDepartamento  
 INNER JOIN Fornecedor FN (NOLOCK)  
  ON ID.IDFornecedor = FN.IDFornecedor  
 LEFT JOIN RelacionamentoItemPrincipal RIP (NOLOCK)  
  ON ID.IDItemDetalhe = RIP.IDItemDetalhe  
 LEFT JOIN RelacionamentoItemSecundario RIS (NOLOCK)  
  ON ID.IDItemDetalhe = RIS.IDItemDetalhe  
  
WHERE  
 ID.cdSistema = @CdSistema  
 AND (DP.IDDepartamento = @IDDepartamento  
 OR @IDDepartamento IS NULL)  
 AND DP.blPerecivel = 'S'  
 AND (ID.tpStatus = @Situacao  
 OR @Situacao IS NULL)  
ORDER BY  
 1  
  , 3</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@CdSistema">
            <Value>=Parameters!CdSistema.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IDDepartamento">
            <Value>=Parameters!IDDepartamento.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Situacao">
            <Value>=Parameters!Situacao.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
