    <add name="WLMSLP" connectionString="Initial Catalog=WLMSLP_PESS_DEV;Data Source=PBRNT56112BR; Application Name=Solução de Perecíveis; User Id=SLPUSER; password=qNpUPj;MultipleActiveResultSets=True;Connection Timeout=60" providerName="System.Data.SqlClient" />
